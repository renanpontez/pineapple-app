<style media="screen">
    body {
        overflow-x: hidden;
    }
</style>
<!--/.Navbar-->
<section id="MainSection">
    <div class="container">
        <div class="" id="Results">
            <div class="row">
                <% _.each(products, function(product) {  %>
                    <% var productLogo = (product.Logo == null) ? '/img/nophoto.png' : 'models/' + product.Logo.photo; %>
                    <div class="col-md-3">
                        <div class="each-result" product-id="<%= product.id %>">
                            <!--Card-->
                            <div class="">
                                <!--Card image-->
                                    <a href="/details/<%= product.id %>" class="photo-section" data-img="<%= productLogo %>">
                                    </a>
                                <!--Card content-->
                                <div class="">
                                    <!--Title-->
                                    <h4 class="title"><%= product.ProductType.name %> <%= product.ProductType.version %> <%= product.storage %>GB</h4>
                                    <!--Text-->
                                    <span class="price">
                                        R$ <%= product.price %>
                                    </span>
                                    <a href="/details/<%= product.id %>" class="btn btn-orange details-btn">DETALHES</a>
                                </div>
                            </div>
                            <!--/.Card-->
                        </div>
                    </div>

                <% }) %>




            </div>
        </div>

    </div>

</section>

<script type="text/javascript">

</script>
