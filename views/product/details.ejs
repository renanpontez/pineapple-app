<div class="container">
    <div class="back-button">
        <a href='#' onclick="if(document.referrer) {window.open(document.referrer,'_self');} else {history.go(-1);} return false;"">
            <i class="fa fa-chevron-left"></i>
        </a>
    </div>
    <div class="row justify-content-md-center">
        <div class="col-md-6">
            <div class="">
                <div class="">
                    <div class="product-details">
                        <div class="product-photo">
                            <img src="/models/<%= product.Logo.photo %>">
                        </div>
                        <div class="product-summary">
                            <div class="model-infos">
                                <div class="each-info">
                                    <span class="master-info">
                                        <%= product.ProductType.name %>
                                        <%= product.ProductType.version %>
                                        <%= product.storage %>GB
                                    </span>
                                </div>
                                <div class="each-info">
                                    <span class="description"><%= product.description %></span>
                                </div>
                                <div class="each-info">
                                    <span class="price">
                                        R$ <%= product.price %>
                                    </span>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12 text-center">
                                <a href="tel:8532440339" class="btn btn-pineapple btn-call">LIGAR</a>
                                <span class="call-info">
                                    Ligue agora: (85) 3244.0339
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
